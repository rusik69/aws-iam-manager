# AWS Credentials for master account
# Set these environment variables or configure AWS CLI with 'aws configure'
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# Optional: IAM Manager cross-account role name (deployed by StackSet)
IAM_ORG_ROLE_NAME=IAMManagerCrossAccountRole

# Optional: Custom names for resources
IAM_USER_NAME=iam-manager
IAM_POLICY_NAME=IAMManagerCustomPolicy
STACK_SET_NAME=IAMManagerRoleStackSet